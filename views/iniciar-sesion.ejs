<main class="contenedor contenedor-formularios element style">
    <h1><%= nombrePagina %></h1>
    <form action="/auth/iniciar-sesion" method="POST" class="default-form" id="formulario">
        <div class="campo">
            <label for="email">E-mail</label>
            <input type="email" name="email"   placeholder="Tu Email">
        </div>
        <div class="campo">
            <label for="password">Password</label>
            <input type="password" name="password"  placeholder="Tu password">
        </div>

        <div class="campo">
            <input type="submit" class="btn btn-rosa" value="Iniciar Sesion">
        </div>
    </form>
</main>




